# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User  {
  id:           Int!
  name:         String!
  age:          Int!
  emailAddress: String!
  gradeName:    String!
}

type Grade {
  id:      Int!
  name:    String!
}

type Query {
  users: [User!]!
}

